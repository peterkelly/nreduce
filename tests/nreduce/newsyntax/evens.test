function printnumbers(lst)
{
  if (lst && (head(lst) >= 0)) {
    numtostring(head(lst));
    "\n";
    printnumbers(tail(lst));
  }
  else
    nil;
}

function main(args)
{
  let numbers = evens(20);
  printnumbers(numbers);
  "fin.\n";
}
