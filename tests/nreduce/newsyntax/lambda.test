function applylist(fun,lst,n)
{
  if (lst) {
    [fun(head(lst),n)];
    applylist(fun,tail(lst),n+1);
  }
  else
    nil;
}

function printstrings(lst,n)
{
  if (lst)
    numtostring(n).": ".head(lst)."\n".printstrings(tail(lst),n+1);
  else
    nil;
}

function main(args)
{
  let fun = lambda (str,index) { "String: \"".str."\"; number ".numtostring(index); };
  let res = applylist(fun,["one","two","three"],1);
  printstrings(res,0);
}
