===================================== FILE =====================================
test.xsl
<?xml version="1.0" encoding="utf-8"?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
		xmlns:test2="../webservices/test2/test2.wsdl"
		version="2.0">

  <xsl:output method="xml" indent="yes"/>
  <xsl:strip-space elements="*"/>

  <xsl:template match="/">
    <result>
      <xsl:for-each select="test2:getNames()/self::*:getNamesReturn">
	<person name="{.}">
	  <xsl:sequence select="test2:getPerson(string(.))/node()"/>
	</person>
      </xsl:for-each>
    </result>
  </xsl:template>

</xsl:stylesheet>
