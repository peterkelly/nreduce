inputstr =
""

times n lst =
(if (== n 0)
  nil
  (append lst (times (- n 1) lst)))

loop services port data =
(if services
    (connect (head services)
                 port
                 (loop (tail services) port data))
    data)

main args =
(if (< (len args) 3)
    "Usage: pipeline-tr.elc kb port services...\n"
    (letrec
      size = (ston (item 0 args))
      port = (ston (item 1 args))
      services = (skip 2 args)
    in
      (++ (loop services port (times size inputstr)) "\nEND\n")))
