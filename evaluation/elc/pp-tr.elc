inputstr =
""

times n lst =
(if (== n 0)
  nil
  (append lst (times (- n 1) lst)))

range from to =
(if (<= from to)
    (cons from (range (+ from 1) to))
    nil)

foldr f base lst =
(if lst
    (f (head lst) (foldr f base (tail lst)))
    base)

addnewline str =
(++ str "\n")

printstrings strings =
(foldr ++ "" (map addnewline strings))

// Usage: nitems comp services...

main args =
(if (< (len args) 5)
  "Usage: pp-tr.elc nitems nstages isize host port\n"
(letrec nitems = (stringtonum (item 0 args))
        nstages = (stringtonum (item 1 args))
        isize = (stringtonum (item 2 args))
        host = (item 3 args)
        port = (stringtonum (item 4 args))

        input = (times isize inputstr)

        callservice x =
          (connect host port x)

        f n x = (if (> n 0)
                    (callservice (f (- n 1) x))
                    x)

        res = (map (f nstages) (map (!x.input) (range 1 nitems)))

 in     (++ (printstrings res) "END\n")))
