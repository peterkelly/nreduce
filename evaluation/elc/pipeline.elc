inputstr =
""

times n lst =
(if (== n 0)
  nil
  (append lst (times (- n 1) lst)))

main args =
(if (< (len args) 4)
  "Please specify 4 hosts"
  (letrec
    host1 = (item 0 args)
    host2 = (item 1 args)
    host3 = (item 2 args)
    host4 = (item 3 args)
    val0 = (times 1024 inputstr)
    val1 = (connect host1 1234 val0)
    val2 = (connect host2 1234 val1)
    val3 = (connect host3 1234 val2)
    val4 = (connect host4 1234 val3)
  in
    val4))
