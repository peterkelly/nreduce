  <xsl:function name="f:test">
    <xsl:apply-templates mode="foo">
      <xsl:with-param name="a" select="4"/>
      <xsl:with-param name="b">
        <xsl:value-of select="2"/>
      </xsl:with-param>
    </xsl:apply-templates>
  </xsl:function>
</xsl:transform>
