===================================== FILE =====================================
test.xsl
<?xml version="1.0"?>
<xsl:transform xmlns:xsl="http://www.w3.org/1999/XSL/Transform" 
  xmlns:g="http://gridxslt.sourceforge.net" exclude-result-prefixes="g" version="2.0">
  <xsl:output indent="yes"/>

  <xsl:function name="g:test">
    <xsl:sequence select="4,5,6"/>
  </xsl:function>

  <xsl:template match="/">
    <items>
      <xsl:for-each select="g:test()">
        <item number="{position()}" of="{last()}"><xsl:value-of select="."/></item>
      </xsl:for-each>
    </items>
  </xsl:template>

</xsl:transform>
