<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:simpleType name="st1" final="restriction">
    <xsd:restriction base="xsd:int">
      <xsd:minExclusive value="4"/>
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>
