A complex type with simple content that extends another complex type that
allows mixed content and has an emptiable particle

=================================== PROGRAM ====================================
testxmlschema -d
==================================== INPUT =====================================
<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:complexType name="ct1">
    <xsd:simpleContent>
      <xsd:restriction base="ct2">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="4"/>
          </xsd:restriction>
        </xsd:simpleType>
        <xsd:maxLength value="8"/>
      </xsd:restriction>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ct2" mixed="true">
    <xsd:sequence>
      <xsd:element name="element1" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
