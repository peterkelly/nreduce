<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:attributeGroup name="ag1">
    <xsd:attribute name="attribute1" type="xsd:ID"/>
    <xsd:attribute name="attribute2" type="xsd:string"/>
  </xsd:attributeGroup>
</xsd:schema>
