=================================== PROGRAM ====================================
nreduce -a addnumbers runtests.tmp/test.l
===================================== FILE =====================================
test.l
T_NUMBER = 1
T_ERROR = 2

add x y =
(if (and (= (head x) T_NUMBER) (= (head y) T_NUMBER))
    (cons T_NUMBER (+ (tail x) (tail y)))
    (cons T_ERROR nil))

addnumbers a b = (add (cons T_NUMBER a) (cons T_NUMBER b))

main = nil
==================================== OUTPUT ====================================
addnumbers a b = (cons 1 (+ a b))
