Test to ensure graph copying detects x even when it's used within a cyclic
definition.
=================================== PROGRAM ====================================
nreduce -a test runtests.tmp/test.l
===================================== FILE =====================================
test.l
test a b c d e = (letrec
                    x = (if a b c)
                    y = (+ (+ x y) y)
                  in
                    (if x
                        (cons x y)
                        (cons y x)))

main = nil
==================================== OUTPUT ====================================
test a b c d e = (letrec 
                    _v0 = (+ (+ c _v0) _v0)
                    _v1 = (+ (+ b _v1) _v1)
                  in
                    (if a 
                        (if b 
                            (cons b _v1) 
                            (cons _v1 b))
                        (if c 
                            (cons c _v0) 
                            (cons _v0 c))))
================================== RETURN CODE =================================
0
