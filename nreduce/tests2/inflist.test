=================================== PROGRAM ====================================
nreduce -a test runtests.tmp/test.l
===================================== FILE =====================================
test.l
producer x = (cons x (producer (+ x 1)))

doubler lst =
(if lst
    (cons (* (head lst) 2)
          (doubler (tail lst)))
    nil)

test x = (doubler (producer x))

main = nil
==================================== OUTPUT ====================================
test x = (cons (* x 2) (test (+ x 1)))
