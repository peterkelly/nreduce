=================================== PROGRAM ====================================
nreduce -a test runtests.tmp/test.l
===================================== FILE =====================================
test.l
producer x =
(if (< x 20)
    (cons x (producer (+ x 1)))
    nil)

doubler lst =
(if lst
    (cons (* (head lst) 2)
          (doubler (tail lst)))
    nil)

totext lst prefix =
(if lst
    (append prefix
            (append (numtostring (head lst))
                    (append "\n"
                            (totext (tail lst) prefix))))
    nil)

test x = (totext (doubler (producer x)) "Number: ")

merge lst =
(if lst
    (append (head lst)
            (merge (tail lst)))
    nil)

main = (test 4)
==================================== OUTPUT ====================================
test x = (if (< x 20) 
             (append "Number: " (append (numtostring (* x 2)) (append "\n" (test (+ x 1))))) 
             nil)
